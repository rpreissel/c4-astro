---
/**
 * LikeC4 View component for the E-Commerce project.
 */
import LikeC4ViewBase from './LikeC4ViewBase.astro'
import { LikeC4View, type LikeC4ViewId } from 'likec4:react/ecommerce'
import { extractPropsWithDefaults, type LikeC4ViewProps } from './likec4-types'

interface Props extends LikeC4ViewProps<LikeC4ViewId> {}

const p = extractPropsWithDefaults(Astro.props)
---

<LikeC4ViewBase>
  <LikeC4View {...p} client:only="react" />
</LikeC4ViewBase>
